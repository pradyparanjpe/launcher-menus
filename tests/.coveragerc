# -*- coding: utf-8; mode: conf; -*-

[report]
exclude_lines =
    pragma: no cover
    if __name__ == '__main__':
    def __repr__
    @nb\.vectorize
    @nb\.njit
    @nb\.jit
ignore_errors = True

[run]
omit =
    */launcher_menus/__main__.py
    */launcher_menus/command_input.py
    */launcher_menus/errors.py
    */launcher_menus/native_parallel.py
source = launcher_menus

[html]
show_contexts = True